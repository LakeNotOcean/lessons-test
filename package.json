{
  "name": "lessons-test",
  "version": "1.0.0",
  "description": "Solution of the test task for the company \"Мой класс\" ",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",

    "build":"swc src -d dist -D",
    "build:watch":"swc src -d dist -D --sync -w",

    "start:debug":"cross-env NODE_ENV=debug node ./src/index.js --inspect",
    "start":"cross-env NODE_ENV=dev node ./dist/index.js",

    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"{src,apps,libs,test}/**/*.js\" --fix"
  },
  "keywords": [
    "test task",
    "Мой Класс"
  ],
  "author": "Nevsky Vladislav",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.90",
    "chokidar": "^3.5.3",
    "eslint": "^8.50.0"
  }
}
